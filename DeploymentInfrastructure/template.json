                                "path": "/datasets/default/files",
                                "queries": {
                                    "folderPath": "/wiredbrainsamplefiles",
                                    "name": "@{body('JSON_analysieren')?['City']}_@{body('JSON_analysieren')?['SerialNumber']}_@{body('JSON_analysieren')?['SensorType']}_@{body('JSON_analysieren')?['SensorValue']}.txt",
                                    "queryParametersSingleEncoded": true
                                }
                            }
                        },
                        "JSON_analysieren": {
                            "runAfter": {},
                            "type": "ParseJson",
                            "inputs": {
